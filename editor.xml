<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="1000" />
    <option name="IS_CARET_BLINKING" value="false" />
    <option name="IS_BLOCK_CURSOR" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="JetEditorOptions">
    <option name="enableJavaToKotlinConversion" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="\bLI\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>